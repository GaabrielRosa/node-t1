{
  "name": "app",
  "version": "1.0.0",
  "versionNPM": "7.21.0",
  "versionNode": "16",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --respawn --transpile-only -r tsconfig-paths/register src/shared/infra/http/index.ts",
    "test": "jest --config jest.units.config.ts",
    "test:debug": "jest --config jest.units.config.ts --debug",
    "test:e2e": "NODE_ENV=test jest --config jest.e2e.config.ts --detectOpenHandles",
    "test:e2e:debug": "NODE_ENV=test jest --config jest.e2e.config.ts --debug",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "cd src/shared/infra/typeorm/migrations/ && npx typeorm migration:create",
    "migration:up": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d src/shared/infra/typeorm/index.ts",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d src/shared/infra/typeorm/index.ts",
    "seed:user": "ts-node-dev -r tsconfig-paths/register src/shared/infra/typeorm/seeds/user.ts",
    "seed:clients": "ts-node-dev -r tsconfig-paths/register src/shared/infra/typeorm/seeds/clients.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "pino": "^8.1.0",
    "rate-limiter-flexible": "^2.3.7",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.6",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "jest": "^27.0.4",
    "supertest": "^6.2.3",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^2.0.0-0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
}
